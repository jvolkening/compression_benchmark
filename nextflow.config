docker.enabled = true
process.executor = 'awsbatch'
process.queue = 'spot_any'
process.container = 'job-definition://nf_priv'
//process.container = 'base2bio/docker-nf:root'
process.errorStrategy = 'retry'
process.maxErrors = 3

// set minimal defaults
process.cpus = 1
process.memory = '4GB'

aws {
    region = 'us-east-2'
}
